    <DocumentationFile>bin\Release\Mono.TextEditor.xml</DocumentationFile>
    <Prefer32Bit>false</Prefer32Bit>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net47|x86'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
